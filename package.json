  },
  "homepage": "https://github.com/eftakhairul/nodejs-api-hack#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-shell": "^0.6.5",
